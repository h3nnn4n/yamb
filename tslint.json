{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-unused-expression": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-snake-case",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    }
  }
}
